{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "noUncheckedIndexedAccess": true,
    "useUnknownInCatchVariables": true,
    "strictNullChecks": true
  },
  "include": ["vite.config.ts"]
}
